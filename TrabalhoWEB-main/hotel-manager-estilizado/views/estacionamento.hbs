<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  {{#if estacionamento.length}}
  <table>
    <thead>
      <tr>
        <th>Veiculo</th>
        <th>placa</th>
      </tr>
    </thead>
    <tbody>
      {{#each estacionamento}}
      <tr>
        <td>{{this.veiculo}}</td>
        <td>{{this.placa}}</td>
        <td>
          <a href="/estacionamento/editar/{{this.id}}"><button type="button">Editar</button></a>
          <form action="/estacionamento/excluir/{{this.id}}" method="POST" style="display:inline;">
            <button type="submit">Excluir</button>
          </form>
        </td>
      </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <p>Nenhuma reserva cadastrada.</p>
{{/if}}

</body>
</html>